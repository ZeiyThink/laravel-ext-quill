{
    "name": "zeiythink/quill",
    "description": "quill editor for php-panel/laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/ZeiyThink/laravel-ext-quill.git",
    "license": "MIT",
    "authors": [
        {
            "name": "zeiythink",
            "email": "zeiythink@gmail.com"
        }
    ],
    "require": {
         "php": "^8.2",
      "php-panel/laravel-admin": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Jxlwqq\\Quill\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jxlwqq\\Quill\\QuillServiceProvider"
            ]

        }
    }
}
